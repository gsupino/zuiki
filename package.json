{
  "name": "zuiki-bakery",
  "description": "Pastry and Food app",
  "author": "Gianluca Supino <gianluca.supino@gmail.com> (http://github.com/gsupino)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/gsupino/zuiki-bakery",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "redux",
    "babel"
  ],
  "main": "babel.server.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config ./webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start": {
      "command": "node ./babel.server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./babel.server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "app-root-path": "^1.0.0",
    "babel": "5.8.21",
    "babel-plugin-typecheck": "1.2.0",
    "classnames": "^2.1.3",
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "file-loader": "^0.8.4",
    "http-proxy": "^1.11.1",
    "isomorphic-fetch": "^2.1.1",
    "material-ui": "^0.10.4",
    "normalizr": "^0.1.3",
    "object-assign": "^3.0.0",
    "piping": "0.2.0",
    "pretty-error": "^1.1.2",
    "radium": "^0.13.7",
    "react": "0.13.3",
    "react-inline-css": "1.2.0",
    "react-redux": "^2.1.2",
    "react-router": "v1.0.0-beta3",
    "react-tap-event-plugin": "^0.1.7",
    "redux": "^3.0.0",
    "redux-devtools": "^2.1.2",
    "redux-thunk": "^0.1.0",
    "reselect": "0.0.2",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.3.0",
    "ua-parser-js": "^0.7.9",
    "url-loader": "^0.5.6"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "5.8.22",
    "babel-eslint": "^4.0.5",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.20",
    "better-npm-run": "0.0.1",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "0.1.1",
    "css-loader": "^0.16.0",
    "css-modules-require-hook": "^1.0.4",
    "eslint": "^0.23.0",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.5.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "gulp": "^3.9.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-nodemon": "^2.0.3",
    "gulp-size": "^2.0.0",
    "gulp-util": "^3.0.6",
    "postcss": "^4.1.16",
    "postcss-bem-linter": "^0.6.0",
    "postcss-custom-properties": "^4.2.0",
    "postcss-import": "^6.2.0",
    "postcss-loader": "^0.5.1",
    "postcss-modules-extract-imports": "0.0.5",
    "postcss-modules-local-by-default": "0.0.12",
    "postcss-modules-scope": "0.0.8",
    "react-a11y": "0.2.6",
    "react-hot-loader": "1.2.8",
    "run-sequence": "^1.1.2",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.11.0",
    "webpack-dev-server": "1.10.1"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
